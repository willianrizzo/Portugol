programa {
  funcao inicio() {
    inteiro x, i, result


        faca
        {
            escreva("Digite um numero: ")
            leia(x)

        }enquanto(x <= 0)

        para(i = 0; i <= 10; i++)
        {
            result = x * i
             escreva("\n" + x + " X " + i + " = " + result)
        }
  }
}
